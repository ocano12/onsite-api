{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "@models": ["./src/models/index"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "./src/**/*"],
    "exclude": ["node_modules"]
}
